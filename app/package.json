{
    "name": "micro-certificate",
    "version": "1.0.0",
    "description": "Micro certificates interation app",
    "engines": {
        "node": ">=12",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "start": "nodemon ./server.js --exec babel-node -e js",
        "test": "nyc mocha --recursive"
    },
    "engineStrict": true,
    "author": "Jason PHam",
    "license": "",
    "dependencies": {
        "body-parser": "^1.19.0",
        "cookie-session": "^1.4.0",
        "express": "^4.17.1",
        "express-validator": "^6.6.1",
        "fabric-ca-client": "2.2.0",
        "fabric-network": "2.2.0",
        "mongodb": "^3.6.0",
        "nodemon": "^2.0.4",
        "pm2": "4.4.1"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-0": "^6.24.1"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
